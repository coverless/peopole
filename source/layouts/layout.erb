<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <link href="https://fonts.googleapis.com/css?family=Lobster|Montserrat" rel="stylesheet" type="text/css">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "peopole" %></title>
    <!-- I don't know if we need "normalize" -->
    <%= stylesheet_link_tag "all", "normalize" %>
    <%= javascript_include_tag  "fittext" %>
  </head>

  <!-- <body class="<%= page_classes %>"> -->
  <body style = "">
    <%= yield %>
  </body>
</html>
